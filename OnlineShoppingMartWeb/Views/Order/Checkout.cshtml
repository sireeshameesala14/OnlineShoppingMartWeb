﻿@{
    ViewData["Title"] = "CheckOut";
}
@using Microsoft.AspNetCore.Http
@inject IHttpContextAccessor HttpContextAccessor
@using OnlineShoppingMartWeb.Utility;
@using OnlineShoppingMartWeb.ResponseModels;

@model CheckoutViewModel

@{
    var userAuth = HttpContextAccessor.HttpContext.Session.GetObjectFromJson<UserAuhDetail>("UserSession");
}
<main id="content" role="main">
    <!-- breadcrumb -->
    <div class="bg-gray-13 bg-md-transparent">
        <div class="container">
            <!-- breadcrumb -->
            <div class="my-md-3">
                <nav aria-label="breadcrumb">
                    <ol class="breadcrumb mb-3 flex-nowrap flex-xl-wrap overflow-auto overflow-xl-visble">
                        <li class="breadcrumb-item flex-shrink-0 flex-xl-shrink-1"><a href="../home/index.html">Home</a></li>
                        <li class="breadcrumb-item flex-shrink-0 flex-xl-shrink-1 active" aria-current="page">Checkout</li>
                    </ol>
                </nav>
            </div>
            <!-- End breadcrumb -->
        </div>
    </div>
    <!-- End breadcrumb -->

    <div class="container">
        <div class="mb-5">
            <h1 class="text-center">Checkout</h1>
        </div>
        @if (@userAuth == null || @userAuth.UserId == 0)
        {
            <!-- Accordion -->
            <div id="shopCartAccordion" class="accordion rounded mb-5">
                <!-- Card -->
                <div class="card border-0">
                    <div id="shopCartHeadingOne" class="alert alert-primary mb-0" role="alert">
                        Returning customer? <a href="#" class="alert-link" data-toggle="collapse" data-target="#shopCartOne" aria-expanded="false" aria-controls="shopCartOne">Click here to login</a>
                    </div>
                    <div id="shopCartOne" class="collapse border border-top-0" aria-labelledby="shopCartHeadingOne" data-parent="#shopCartAccordion" style="">
                        <div class="col-md-5  mr-md-auto mr-xl-0 mb-8 mb-md-0" style="float:left;margin-left:376px;">
                            <!-- Form -->
                            @using (@Html.BeginForm("Checkout", "Order"))
                            {
                                Html.RenderPartial("../User/Partials/_SignIn", Model.Login);
                            }
                            <!-- End Form -->
                        </div>
                    </div>
                </div>
                <!-- End Card -->
            </div>
            <!-- End Accordion -->
        }
        @using (@Html.BeginForm("ProcessPayment", "Order"))
        {
            @*   <form class="js-validate" action="/Order/OrderSuccess" method="post"> *@
            <div class="row">
                <div class="col-lg-5 order-lg-2 mb-7 mb-lg-0">
                    <div class="pl-lg-3 ">
                        <div class="bg-gray-1 rounded-lg">
                            <!-- Order Summary -->
                            <div class="p-4 mb-4 checkout-table">
                                <!-- Title -->
                                <div class="border-bottom border-color-1 mb-5">
                                    <h3 class="section-title mb-0 pb-2 font-size-25">Your order</h3>
                                </div>
                                <!-- End Title -->
                                <!-- Product Content -->
                                @if (Model.Products != null)
                                {
                                    <table class="table">
                                        <thead>
                                            <tr>
                                                <th class="product-name">Product</th>
                                                <th class="product-total">Total</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @{
                                                decimal subTotal = 0;
                                                decimal totalProductAmount = 0;
                                                decimal totalTax = 0;
                                            }
                                            @foreach (var product in Model.Products)
                                            {
                                                var total = (product.ProductPrice + product.Tax) * Convert.ToInt32(product.SelectedProductQuantity);
                                                subTotal = subTotal + total;
                                                totalProductAmount = totalProductAmount + (product.ProductPrice * Convert.ToInt32(product.SelectedProductQuantity));
                                                totalTax = totalTax + product.Tax * Convert.ToInt32(product.SelectedProductQuantity);
                                                <tr class="cart_item">
                                                    <td>@product.ProductName<strong class="product-quantity">× @product.SelectedProductQuantity</strong></td>
                                                    <td>$@total.ToString("N2")</td>
                                                </tr>
                                            }
                                            <input type="hidden" name="TotalProductAmount" value="@totalProductAmount" />

                                            <input type="hidden" name="TotalTax" value="@totalTax" />
                                        </tbody>
                                        <tfoot>
                                            <tr>
                                                <th>Subtotal</th>
                                                <td>$@subTotal.ToString("N2")</td>
                                            </tr>
                                            <tr>
                                                <th>Shipping</th>
                                                <td>Flat rate $ @Configuration.ShippingCharge.ToString("N2")</td>
                                            </tr>
                                            <tr>
                                                @{
                                                    var grandTotal = (@subTotal + @Configuration.ShippingCharge);
                                                }
                                                <input type="hidden" name="TotalShipping" value="@Configuration.ShippingCharge" />
                                                <input type="hidden" name="TotalOrderPrice" value="@grandTotal" />
                                                <th>Total</th>
                                                <td><strong>$@grandTotal.ToString("N2")</strong></td>
                                            </tr>
                                        </tfoot>
                                    </table>
                                }
                                <!-- End Product Content -->
                                <div class="border-top border-width-3 border-color-1 pt-3 mb-3">
                                    <!-- Basics Accordion -->
                                    <div id="basicsAccordion1">
                                        <!-- Card -->
                                        <div class="border-bottom border-color-1 border-dotted-bottom">
                                            <div class="p-3" id="basicsHeadingThree">
